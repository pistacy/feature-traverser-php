{
    "name": "pistacy/feature-traverser-php",
    "description": "A library for traversing PHP code dependencies starting from a specific entry point",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "nikic/php-parser": "^4.19"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Pistacy\\FeatureTraverser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pistacy\\FeatureTraverser\\Tests\\": "tests/unit/",
            "Pistacy\\FeatureTraverser\\Tests\\Resources\\Fixtures\\": "tests/resources/fixtures/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
